{
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.integrated.allowChords": false,
  "testMate.cpp.test.advancedExecutables": [
    {
      "pattern": "./bazel-out/k8-dbg/**/*test.runfiles/**/*test",
      "cwd": "${workspaceDirectory}"
    }
  ],
  "bazel-cpp-tools.compileCommands.targets": [
    "//...",
  ],
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "clang-format.executable": "/usr/bin/clang-format",
  "C_Cpp.default.intelliSenseMode": "linux-clang-x64",
  "terminal.integrated.scrollback": 100000,
  "editor.detectIndentation": false,
  "bsv.bzl.lsp.enableCodelenses": true,
  "bsv.bzl.lsp.enableCodelensCopyLabel": true,
  "bsv.bzl.lsp.enableCodelensCodesearch": true,
  "bsv.bzl.lsp.enableCodelensBrowse": true,
  "bsv.bzl.lsp.enableCodelensStarlarkDebug": true,
  "bsv.bzl.lsp.enableCodelensBuild": true,
  "bsv.bzl.lsp.enableCodelensTest": true,
  "bsv.bzl.lsp.enableCodelensRun": true,
